            "scripts": [
                {
                    "label": "超额命中净化",
                    "trigger": "hit",
                    "active": true,
                    "script": "if (args.isHit && args.damageResult && args.damageResult.rollJSON) {\n    const rollData = JSON.parse(args.damageResult.rollJSON);\n    // damageResult.targetsResultMap 里的 key 是 token uuid 的 safeKey，这里直接取 targetsResults 里的\n    // 这里的 args 在 hit 阶段对每个 target 运行，所以 args.target 是当前目标\n    const targetDodge = args.target.system.combat.dodgeTotal || 0;\n    \n    // 注意：rollData.total 是总值\n    if ((rollData.total - targetDodge) >= 10) {\n        ui.notifications.info(`${thisItem.name} 触发：命中超过10点！你可以移除自身一层状态。`);\n    }\n}"
                }
            ]
        },
        "effects": []
